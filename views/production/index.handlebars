<!-- page content -->
<div class="right_col" role="main">
  <!-- top tiles -->
  <div class="row tile_count">
      {{#unless user.municipality}}
        <div class="col-md-4 col-sm-4 col-xs-12 tile_stats_count">
          <span class="count_top"><i class="fa fa-bank"></i> Token bankrekening:</span>
          <div class="count">{{user.bankAccount}}</div>
        </div>
      {{/unless}}
      {{#if user.subsidyAccount}}
        <div class="col-md-4 col-sm-4 col-xs-12 tile_stats_count">
          <span class="count_top"><i class="fa fa-bank"></i> Subsidie bankrekening:</span>
          <div class="count">{{user.subsidyAccount}}</div>
        </div>
      {{/if}}
      {{#if user.kid}}
            <div class="col-md-4 col-sm-4 col-xs-12 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Beschikbaar aantal uur (1 token p/m):</span>
              <div class="count">
                  <script>
                      var bankAccount = Number({{user.bankAccount}});
                      var beschikbareTijd = bankAccount/60;
                      document.write(Math.ceil(beschikbareTijd));
                  </script>
              </div>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12 tile_stats_count">
              <span class="count_top"><i class="fa fa-child"></i> Lessen bijgewoond:</span>
              <div class="count green">{{user.gevolgdeLessen}}&nbsp;</div>
            </div>
      {{/if}}

  <br />
